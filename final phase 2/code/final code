#define sensor1 2
#define sensor2 3
#define sensor3 4
#define M1A 6
#define M1B 7

#define M2A 8
#define M2B 9


#define EN1 5
#define EN2 10
# define echo 12
#define trig 13

void setup()
{
  pinMode(echo,OUTPUT);
 pinMode(trig,OUTPUT);
 pinMode(M1A,OUTPUT);
 pinMode(M1B,OUTPUT);
 pinMode(M2A,OUTPUT);
 pinMode(M2B,OUTPUT);
 pinMode(EN1,OUTPUT);
 pinMode(EN2,OUTPUT);
 pinMode(sensor1,INPUT);
 pinMode(sensor2,INPUT);
 pinMode(sensor3,INPUT);
 Serial.begin(9600);
 
  }
